{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "copy-env": {
      "dependsOn": [
        "^copy-env"
      ],
      "outputs": [
        ".env"
      ],
      "cache": false
    },
    "create-images-domain": {
      "dependsOn": [
        "^create-images-domain"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^copy-env",
        "^create-images-domain",
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "cache": false
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "tests/**/*.test.ts"
      ],
      "outputs": []
    },
    "watch": {
      "cache": false,
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "dependsOn": [
        "^watch"
      ]
    }
  },
  "globalDependencies": [
    "**/.env"
  ]
}