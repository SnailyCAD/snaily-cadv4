{
  "name": "@snailycad/api",
  "version": "1.0.0-beta.64",
  "scripts": {
    "dev": "prisma migrate dev && prisma generate && nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node -r tsconfig-paths/register src/main.ts",
    "start": "yarn workspace @snailycad/schemas build && yarn workspace @snailycad/config build && npx prisma migrate deploy && npx prisma generate && ts-node -r tsconfig-paths/register src/main.ts",
    "start:docker": "yarn add -D esbuild esbuild-linux-64 && yarn workspace @snailycad/schemas build && yarn workspace @snailycad/config build && npx prisma migrate deploy && prisma generate &&  ts-node -r tsconfig-paths/register src/main.ts",
    "format": "prisma format",
    "generate": "prisma generate",
    "typecheck": "yarn run generate && tsc --noEmit",
    "copy-env": "node ../../scripts/copy-env.mjs --api"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "^17.0.21",
    "@types/qrcode": "^1.4.2",
    "esbuild": "^0.14.23",
    "esbuild-register": "^3.3.2",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@prisma/client": "^3.10.0",
    "@snailycad/config": "1.0.0-beta.64",
    "@snailycad/schemas": "1.0.0-beta.64",
    "@snailycad/types": "1.0.0-beta.64",
    "@snailycad/utils": "1.0.0-beta.64",
    "@tsed/common": "^6.103.2",
    "@tsed/core": "^6.103.2",
    "@tsed/di": "^6.103.2",
    "@tsed/exceptions": "^6.103.2",
    "@tsed/json-mapper": "^6.103.2",
    "@tsed/platform-express": "^6.103.2",
    "@tsed/schema": "^6.103.2",
    "@tsed/socketio": "^6.103.2",
    "@tsed/swagger": "^6.103.2",
    "bcrypt": "^5.0.1",
    "compression": "1.7.4",
    "cookie": "^0.4.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "discord-api-types": "0.27.3",
    "express": "4.17.3",
    "glob": "^7.2.0",
    "jsonwebtoken": "8.5.1",
    "nanoid": "^3.3.1",
    "nodemon": "^2.0.15",
    "otplib": "^12.0.1",
    "prisma": "^3.10.0",
    "qrcode": "^1.5.0",
    "socket.io": "^4.4.1",
    "undici": "^4.14.1"
  }
}
