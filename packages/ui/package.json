{
  "name": "@snailycad/ui",
  "version": "1.27.0",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn tsup",
    "watch": "yarn tsup --watch --silent=false",
    "typecheck": "yarn tsc --noEmit"
  },
  "devDependencies": {
    "@snailycad/types": "workplace:*",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vitest": "^0.23.4"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "format": [
      "esm"
    ],
    "external": [
      "react"
    ]
  },
  "dependencies": {
    "@react-aria/button": "^3.6.2",
    "@react-aria/combobox": "^3.4.2",
    "@react-aria/progress": "^3.3.2",
    "@react-aria/select": "^3.8.2",
    "@react-aria/textfield": "^3.7.2",
    "@react-aria/utils": "^3.14.0",
    "@react-stately/collections": "^3.4.4",
    "@react-stately/list": "^3.5.4",
    "@react-stately/select": "^3.3.2",
    "@react-stately/selection": "^3.11.0",
    "next-intl": "^2.7.6",
    "react-bootstrap-icons": "^1.9.1"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
