{
  "name": "@snailycad/schemas",
  "version": "1.73.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./dist/*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm tsup",
    "watch": "pnpm tsup --watch --silent=false",
    "typecheck": "pnpm tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "6.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@snailycad/types": "workspace:*",
    "tslib": "^2.6.2",
    "zod": "3.22.2"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "treeshake": true,
    "silent": true,
    "minify": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "sideEffects": false
}
