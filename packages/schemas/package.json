{
  "name": "@snailycad/schemas",
  "version": "1.9.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./dist/*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn tsup",
    "typecheck": "yarn tsc --noEmit"
  },
  "devDependencies": {
    "tslib": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "zod": "^3.17.3"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "minify": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
