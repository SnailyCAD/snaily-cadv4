{
  "name": "@snailycad/utils",
  "version": "1.0.0-beta.78",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./callsign": "./dist/callsign.js",
    "./typeguards": "./dist/typeguards.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "callsign": [
        "dist/callsign.d.ts"
      ],
      "typeguards": [
        "dist/typeguards.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test:watch": "vitest --watch",
    "test": "vitest run"
  },
  "devDependencies": {
    "@snailycad/types": "1.0.0-beta.78",
    "c8": "^7.11.0",
    "tslib": "^2.3.1",
    "tsup": "^5.12.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vitest": "^0.8.1"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "minify": true
  }
}
