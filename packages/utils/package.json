{
  "name": "@snailycad/utils",
  "version": "1.15.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./callsign": {
      "require": "./dist/callsign.js",
      "import": "./dist/callsign.mjs"
    },
    "./typeguards": {
      "require": "./dist/typeguards.js",
      "import": "./dist/typeguards.mjs"
    },
    "./version": {
      "require": "./dist/version.js",
      "import": "./dist/version.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "callsign": [
        "dist/callsign.d.ts"
      ],
      "typeguards": [
        "dist/typeguards.d.ts"
      ],
      "version": [
        "dist/version.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "yarn tsup",
    "watch": "yarn tsup --watch --silent=false",
    "typecheck": "yarn tsc --noEmit",
    "test:watch": "yarn vitest --watch",
    "test": "yarn vitest run"
  },
  "devDependencies": {
    "@snailycad/types": "1.15.1",
    "c8": "^7.11.3",
    "tslib": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.16.0"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
