{
  "name": "@snailycad/types",
  "version": "1.2.1",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn tsup",
    "typecheck": "yarn tsc --noEmit"
  },
  "dependencies": {
    "type-fest": "^2.12.2",
    "zod": "^3.16.0"
  },
  "devDependencies": {
    "@snailycad/permissions": "1.2.1",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
