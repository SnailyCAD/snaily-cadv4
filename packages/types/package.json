{
  "name": "@snailycad/types",
  "version": "1.27.1",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./api": {
      "require": "./dist/api.js",
      "import": "./dist/api.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "api": [
        "dist/api.d.ts"
      ]
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn workspace @snailycad/api generate && yarn tsup",
    "watch": "yarn tsup --watch --silent=false",
    "typecheck": "yarn workspace @snailycad/api generate && yarn tsc --noEmit"
  },
  "dependencies": {
    "type-fest": "^3.0.0"
  },
  "devDependencies": {
    "@snailycad/permissions": "1.27.1",
    "discord-api-types": "0.37.11",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "tsup": {
    "entry": [
      "./src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
