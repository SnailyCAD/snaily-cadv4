{
  "name": "@snailycad/config",
  "version": "1.6.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn tsup",
    "typecheck": "yarn tsc --noEmit",
    "test:watch": "yarn vitest --watch",
    "test": "yarn vitest run"
  },
  "devDependencies": {
    "@snailycad/types": "1.6.1",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.13.1"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/routes.ts",
      "src/socket-events.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
