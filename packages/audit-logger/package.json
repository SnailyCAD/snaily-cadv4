{
  "name": "@snailycad/audit-logger",
  "version": "1.13.1",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "require": "./dist/core/server.js",
      "import": "./dist/core/server.mjs"
    },
    "./client": {
      "require": "./dist/core/client.js",
      "import": "./dist/core/client.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn tsup",
    "watch": "yarn tsup --watch --silent=false",
    "typecheck": "yarn tsc --noEmit"
  },
  "dependencies": {
    "@snailycad/utils": "1.13.1",
    "superjson": "^1.9.1",
    "type-fest": "^2.13.0"
  },
  "devDependencies": {
    "@snailycad/types": "1.13.1",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "zod": "3.17.3"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts",
      "src/core/server.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "typesVersions": {
    "*": {
      "server": [
        "dist/core/server.d.ts"
      ],
      "client": [
        "dist/core/client.d.ts"
      ]
    }
  }
}
