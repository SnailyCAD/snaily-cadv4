{
  "name": "@snailycad/audit-logger",
  "version": "1.12.0",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn tsup",
    "watch": "yarn tsup --watch --silent=false",
    "typecheck": "yarn tsc --noEmit"
  },
  "dependencies": {
    "superjson": "^1.9.1",
    "type-fest": "^2.13.0"
  },
  "devDependencies": {
    "@snailycad/types": "1.12.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
