{
  "name": "@snailycad/audit-logger",
  "version": "1.52.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx rimraf ./dist && yarn workspace @snailycad/api generate && yarn tsup",
    "watch": "yarn tsup --watch --silent=false",
    "typecheck": "yarn tsc --noEmit"
  },
  "devDependencies": {
    "@snailycad/types": "1.52.0",
    "tsup": "^6.6.3",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "treeshake": true,
    "target": "node16",
    "silent": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "sideEffects": false,
  "dependencies": {
    "superjson": "^1.12.2"
  },
  "typesVersions": {
    "*": {
      "server": [
        "dist/server.d.ts"
      ]
    }
  }
}
