{
  "name": "@snailycad/permissions",
  "version": "1.0.0-beta.77",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test:watch": "vitest --watch",
    "test": "vitest run"
  },
  "devDependencies": {
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vitest": "^0.8.1"
  },
  "dependencies": {
    "type-fest": "^2.12.1",
    "zod": "^3.14.3"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true
  },
  "exports": {
    ".": "./dist/index.js",
    "./permissions": "./dist/permissions.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "permissions": [
        "dist/permissions.d.ts"
      ]
    }
  }
}
