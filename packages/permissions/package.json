{
  "name": "@snailycad/permissions",
  "version": "1.0.0-beta.68",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test:watch": "vitest --watch",
    "test": "vitest run"
  },
  "devDependencies": {
    "tsup": "^5.11.13",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  },
  "dependencies": {
    "type-fest": "^2.12.0",
    "zod": "^3.12.0"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "dts": true,
    "bundle": false,
    "platform": "node",
    "target": "node16",
    "silent": true
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./permissions": {
      "default": "./dist/permissions.js",
      "types": "./dist/permissions.d.ts"
    }
  }
}
