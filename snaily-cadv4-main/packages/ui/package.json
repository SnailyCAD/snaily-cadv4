    "tsup": "6.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.2.13"
  },
  "tsup": {
    "entry": [
      "src/index.tsx",
      "src/**/*.tsx",
      "src/**/*.ts",
      "!src/**/*.stories.tsx",
      "!src/stories/**/*"
    ],
    "bundle": false,
    "dts": true,
    "platform": "node",
    "target": "node18",
    "silent": true,
    "treeshake": true,
    "format": [
      "esm"
    ]
  },
