{
  "name": "@snailycad/ui",
  "type": "module",
  "version": "1.80.2",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm tsup",
    "watch": "pnpm tsup --watch --silent=false",
    "typecheck": "pnpm tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --webpack-stats-json",
    "chromatic": "pnpm chromatic",
    "prepublish": "pnpm build",
    "test:e2e": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"http-server storybook-static --port 6006 --silent\" \"wait-on http-get://127.0.0.1:6006 -v && pnpm test-storybook\""
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-themes": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/test-runner": "^0.15.2",
    "@storybook/testing-library": "^0.2.2",
    "autoprefixer": "^10.4.19",
